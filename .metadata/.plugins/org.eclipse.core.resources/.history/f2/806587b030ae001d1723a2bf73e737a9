
public class AlunoEnsinoSuperior extends Aluno {
	
	public double getTeste() {
		return teste;
	}


	public void setTeste(double teste) {
		this.teste = teste;
	}


	public double getProva() {
		return prova;
	}


	public void setProva(double prova) {
		this.prova = prova;
	}


	double getMedia() {
		return (teste + prova*2)/3;
	}
}
